public class countingzeors {
    public static void main(String[] args) {
        int []arr={0, 0, 0, 0, -4,88,0,9,0,0};
        int count=0;
        System.out.println("0's are ");
        for(int i=0;i<arr.length;i++){
            if(arr[i]<1&&arr[i]>-1){
                count++;
            }

        }
        System.out.print(count);
    }
}
